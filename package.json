    "module": "rollup -c && cp -r dist/* ../otrm-starter/node_modules/otrm/dist && cp dist/otrm.bundle.* example",
    "local": "http-server ./example"
  },
  "peerDependencies": {
    "react": ">= 17.0",
    "react-dom": ">= 17.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
