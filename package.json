    "handlebars": "^4.7.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.0",
    "react-router-dom": "^5.2.0",
    "react-youtube": "^7.13.1",
    "rehype-raw": "^5.1.0",
    "remark-gfm": "^1.0.0",
    "remark-highlight.js": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
