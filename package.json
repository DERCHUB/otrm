{
  "id": "otrm",
  "name": "otrm",
  "version": "0.1.22",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/derchub/otrm"
  },
  "umd": "dist/otrm.bundle.js",
  "module": "dist/otrm.es.js",
  "jsnext:main": "dist/otrm.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
